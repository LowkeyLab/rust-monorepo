html_body: "<div class=\"bg-base-200 p-4 rounded-lg\">\n  <h3 class=\"text-lg font-semibold mb-3\">Add New Name</h3>\n  <div id=\"error-message\" class=\"mb-4\"></div>\n  <form\n    hx-post=\"/names\"\n    hx-target=\"#names-table\"\n    hx-swap=\"outerHTML\"\n    hx-on::after-request=\"if(event.detail.xhr.status === 200) { document.getElementById('add-name-form').innerHTML = ''; document.getElementById('error-message').innerHTML = ''; }\"\n    class=\"space-y-4\"\n  >\n    <div class=\"form-control\">\n      <label class=\"label\">\n        <span class=\"label-text\">Discord ID</span>\n      </label>\n      <input\n        type=\"number\"\n        name=\"discord_id\"\n        placeholder=\"Enter Discord ID\"\n        class=\"input input-bordered w-full\"\n        required\n      />\n    </div>\n    <div class=\"form-control\">\n      <label class=\"label\">\n        <span class=\"label-text\">Name</span>\n      </label>\n      <input\n        type=\"text\"\n        name=\"name\"\n        placeholder=\"Enter name\"\n        class=\"input input-bordered w-full\"\n        required\n      />\n    </div>\n    <div class=\"flex gap-2\">\n      <button type=\"submit\" class=\"btn btn-primary\">\n        <svg\n          xmlns=\"http://www.w3.org/2000/svg\"\n          class=\"h-4 w-4 mr-2\"\n          fill=\"none\"\n          viewBox=\"0 0 24 24\"\n          stroke=\"currentColor\"\n        >\n          <path\n            stroke-linecap=\"round\"\n            stroke-linejoin=\"round\"\n            stroke-width=\"2\"\n            d=\"M5 13l4 4L19 7\"\n          />\n        </svg>\n        Add Name\n      </button>\n      <button\n        type=\"button\"\n        class=\"btn btn-ghost\"\n        onclick=\"document.getElementById('add-name-form').innerHTML = ''\"\n      >\n        Cancel\n      </button>\n    </div>\n  </form>\n</div>"
